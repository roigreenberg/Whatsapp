class EchoLayer(YowInterfaceLayer):
    @ProtocolEntityCallback("message")
    def onMessage(self, message):     
        if (message.isGroupMessage()):
            print("(GROUP)[%s]-[%s]\t%s" % (message.getParticipant(), message.getFrom(), message.getBody()))
        else:
            print("(PVT)[%s]\t%s" % (message.getFrom(), message.getBody()))

